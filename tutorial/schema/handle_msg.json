{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandleMsg",
  "description": "A message sent to transfer funds and collect fees for a purchase.",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "purchase"
      ],
      "properties": {
        "purchase": {
          "type": "object",
          "required": [
            "id"
          ],
          "properties": {
            "id": {
              "type": "string"
            }
          }
        }
      }
    }
  ]
}
